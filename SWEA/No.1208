#1208(10ë¶„)

testcase = 10
for t in range(1, testcase+1):
    num = int(input())
    box = list(map(int, input().split()))
    for _ in range(num):
        max_loc = box.index(max(box))
        min_loc = box.index(min(box))

        box[max_loc] = box[max_loc] - 1
        box[min_loc] = box[min_loc] + 1

    cnt = max(box) - min(box)
    print(f"#{t} {cnt}")
