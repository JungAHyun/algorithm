#1213(10ë¶„)

T = 10

for t in range(1, T+1):
    num = int(input())
    find_str = input()
    string = input()

    index = string.find(find_str)
    cnt = 0

    while(index > 0):
        string = string[0:index] + string[index+len(find_str):]
        cnt += 1
        index = string.find(find_str)

    print(f"#{t} {cnt}")
